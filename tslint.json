{
  "defaultSeverity": "error",
  "extends": [
    "tslint-config-airbnb",
    "tslint-react",
    "tslint-plugin-prettier"
  ],
  "jsRules": {},
  "rules": {
    "prettier": [
      true,
      {
        "singleQuote": true,
        "trailingComma": "all"
      }
    ],
    "variable-name": [true, "allow-pascal-case"],
    "interface-name": [true, "always-prefix"],
    "ter-arrow-parens": false,
    "jsx-no-lambda": false,
    "no-unused-variable": true
  },
  "rulesDirectory": []
}
